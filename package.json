{
  "name": "script-load",
  "description": "Given a URL, asynchronously load a script in the browser.",
  "version": "0.0.0",
  "main": "index.js",
  "browser": "index.js",
  "dependencies": {
    "once": "~1.3.0"
  },
  "devDependencies": {
    "tape": "~2.1.0",
    "browserify": "~2.35.1",
    "testling": "~1.5.3"
  },
  "scripts": {
    "test": "testling"
  },
  "author": "Hugh Kennedy <hughskennedy@gmail.com> (http://github.com/hughsk)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/hughsk/script-load"
  },
  "bugs": {
    "url": "https://github.com/hughsk/script-load/issues"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "script",
    "loading",
    "asynchronous",
    "lazy"
  ],
  "testling": {
    "files": "test/test.js",
    "browsers": [
      "ie/8..10",
      "ff/15..latest",
      "safari/4..latest",
      "android-browser/6",
      "chrome/latest",
      "iphone/6",
      "ipad/6"
    ]
  }
}
